{
  "name": "escape",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "ionic serve",
    "start:prod": "ionic serve --prod",
    "build": "ionic build --stats-json --base-href . && npm run ngsw && ionic cap copy",
    "build:prod": "ionic build --prod --aot --build-optimizer --base-href . && ionic cap copy",
    "deploy": "npm run build && wrangler publish",
    "deploy:prod": "npm run build:prod && wrangler publish",
    "ngsw": "npx ngsw-config www ngsw-config.json && cpx node_modules/@angular/service-worker/ngsw-worker.js www",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "android": "npm run build && cd android && gradlew assembleDebug",
    "android:prod": "npm run build:prod && cd android && gradlew assembleDebug",
    "android:run": "npm run build && ionic cap open android",
    "android:run:prod": "npm run build:prod && ionic cap open android",
    "ios": "npm run build && ionic cap open ios",
    "ios:prod": "npm run build:prod && ionic cap open ios",
    "electron": "npm run build && ionic cap open electron",
    "electron:windows": "npm run build && cd electron && npm run electron:windows",
    "electron:mac": "npm run build && cd electron && npm run electron:mac",
    "electron:linux": "npm run build && cd electron && npm run electron:linux",
    "electron:prod": "npm run build:prod && ionic cap open electron",
    "update:check": "ng update && npm outdated",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points && ionic cap update"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~9.0.7",
    "@angular/core": "~9.0.7",
    "@angular/fire": "^5.4.2",
    "@angular/forms": "~9.0.7",
    "@angular/platform-browser": "~9.0.7",
    "@angular/platform-browser-dynamic": "~9.0.7",
    "@angular/router": "~9.0.7",
    "@angular/service-worker": "~9.0.7",
    "@capacitor/android": "^1.5.1",
    "@capacitor/core": "1.5.1",
    "@capacitor/ios": "^1.5.1",
    "@fortawesome/fontawesome-free": "^5.12.1",
    "@ionic-native/core": "^5.0.0",
    "@ionic-native/splash-screen": "^5.0.0",
    "@ionic-native/status-bar": "^5.0.0",
    "@ionic/angular": "^5.0.0",
    "bootstrap": "^4.4.1",
    "core-js": "^2.5.4",
    "electron-push-receiver": "^2.1.3",
    "firebase": ">= 5.5.7 <8",
    "localforage": "^1.7.3",
    "moment": "^2.24.0",
    "rxjs": "~6.5.1",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "<0.900 || ^0.900.0-0 || ^9.0.0-0",
    "@angular-devkit/build-angular": "~0.901.0-next.4",
    "@angular/cli": "~9.1.0-next.4",
    "@angular/compiler": "~9.0.7",
    "@angular/compiler-cli": "~9.0.7",
    "@angular/language-service": "~9.0.7",
    "@capacitor/cli": "1.5.1",
    "@ionic/angular-toolkit": "^2.1.1",
    "@ionic/cli": "^6.3.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "cpx": "^1.5.0",
    "electron-builder": "^22.4.1",
    "firebase-tools": "^7.12.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "^3.7.5"
  },
  "description": "An Ionic project"
}
